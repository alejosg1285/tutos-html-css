<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cliente.udemyandroidcero">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".App.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_checklist"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_checklist"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.ListViewActivity" />
        <activity android:name=".Activities.RecyclerViewActivity" />
        <activity android:name=".Activities.RealmActivity"></activity>
        <activity android:name=".Activities.NoteActivity"></activity>
    </application>

</manifest>