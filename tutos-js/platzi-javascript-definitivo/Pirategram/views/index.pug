html
    head
        meta(charset='UTF-8')
        meta(name='viewport' content='width=device-width, initial-scale=1.0, user-scalable=no')
        
        link(rel='apple-touch-icon', sizes='57x57', href='images/apple-icon-57x57.png')
        link(rel='apple-touch-icon', sizes='60x60', href='images/apple-icon-60x60.png')
        link(rel='apple-touch-icon', sizes='72x72', href='images/apple-icon-72x72.png')
        link(rel='apple-touch-icon', sizes='76x76', href='images/apple-icon-76x76.png')
        link(rel='apple-touch-icon', sizes='114x114', href='images/apple-icon-114x114.png')
        link(rel='apple-touch-icon', sizes='120x120', href='imagesimages/apple-icon-120x120.png')
        link(rel='apple-touch-icon', sizes='144x144', href='images/apple-icon-144x144.png')
        link(rel='apple-touch-icon', sizes='152x152', href='images/apple-icon-152x152.png')
        link(rel='apple-touch-icon', sizes='180x180', href='images/apple-icon-180x180.png')
        link(rel='icon', type='image/png', sizes='192x192', href='images/android-icon-192x192.png')
        link(rel='icon', type='image/png', sizes='32x32', href='images/favicon-32x32.png')
        link(rel='icon', type='image/png', sizes='96x96', href='images/favicon-96x96.png')
        link(rel='icon', type='image/png', sizes='16x16', href='images/favicon-16x16.png')
        link(rel='manifest', href='/manifest.json')
        meta(name='msapplication-TileColor', content='#ffffff')
        meta(name='msapplication-TileImage', content='images/ms-icon-144x144.png')
        meta(name='theme-color', content='#ffffff')

        // Import Google Icon Font
        link(href='https://fonts.googleapis.com/icon?family=Material+Icons' rel='stylesheet')
        // Tipo de fuente Ubuntu
        link(href='https://fonts.googleapis.com/css?family=Ubuntu|Montserrat+Alternates:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i' rel='stylesheet')
        // Import CDN FontAwesome
        link(rel='stylesheet' href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css')
        link(rel='stylesheet' href='css/app.css')
        title=title
    body
        section#header-container
        section#main-container
        
        // Import jQuery before materialize.js
        script(type='application/javascript', src='https://code.jquery.com/jquery-3.2.1.min.js')
        // Compiled and minified JavaScript
        script(src='https://cdnjs.cloudflare.com/ajax/libs/materialize/0.100.2/js/materialize.min.js')
        script(type='application/javascript', src='js/app.js')